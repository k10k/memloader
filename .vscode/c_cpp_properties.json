{
    "configurations": [
        {
            "name": "ARM",
            "intelliSenseMode": "clang-x64",
            "includePath": [
                "${DEVKITARM}/arm-none-eabi/include",
                "${DEVKITARM}/lib/gcc/arm-none-eabi/8.1.0/include",
                "${workspaceFolder}/src"
            ],
            "defines": [
                "NDEBUG",
                "DEBUG_UART_PORT=UART_A"
            ],
            "compilerPath": "\"${DEVKITARM}/bin/arm-none-eabi-gcc${EXECUTABLE_SUFFIX}\" -march=armv4t -mtune=arm7tdmi -mthumb -mthumb-interwork",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}